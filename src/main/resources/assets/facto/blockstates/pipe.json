{
  "multipart": [
    {
      "apply": {
        "model": "facto:block/pipe_center"
      }
    },
    {
      "when": {
        "north": "connected"
      },
      "apply": {
        "model": "facto:block/pipe_end"
      }
    },
    {
      "when": {
        "south": "connected"
      },
      "apply": {
        "model": "facto:block/pipe_end",
        "y": 180
      }
    },
    {
      "when": {
        "east": "connected"
      },
      "apply": {
        "model": "facto:block/pipe_end",
        "y": 90
      }
    },
    {
      "when": {
        "west": "connected"
      },
      "apply": {
        "model": "facto:block/pipe_end",
        "y": 270
      }
    },
    {
      "when": {
        "down": "connected"
      },
      "apply": {
        "model": "facto:block/pipe_end",
        "x": 90
      }
    },
    {
      "when": {
        "up": "connected"
      },
      "apply": {
        "model": "facto:block/pipe_end",
        "x": 270
      }
    },
    {
      "when": {
        "north": "external"
      },
      "apply": {
        "model": "facto:block/pipe_end_external"
      }
    },
    {
      "when": {
        "south": "external",
        "module": "none"
      },
      "apply": {
        "model": "facto:block/pipe_end_external",
        "y": 180
      }
    },
    {
      "when": {
        "south": "external",
        "module": "extract"
      },
      "apply": {
        "model": "facto:block/pipe_end_external",
        "y": 180
      }
    },
    {
      "when": {
        "east": "external"
      },
      "apply": {
        "model": "facto:block/pipe_end_external",
        "y": 90
      }
    },
    {
      "when": {
        "west": "external"
      },
      "apply": {
        "model": "facto:block/pipe_end_external",
        "y": 270
      }
    },
    {
      "when": {
        "down": "external"
      },
      "apply": {
        "model": "facto:block/pipe_end_external",
        "x": 90
      }
    },
    {
      "when": {
        "up": "external"
      },
      "apply": {
        "model": "facto:block/pipe_end_external",
        "x": 270
      }
    }
  ]
}